{
  "name": "property-manager",
  "version": "0.0.0-baseline-0",
  "private": true,
  "main": "electron/main.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently -k -r \"vite\" \"npm:electron:dev\"",
    "electron:dev": "wait-on tcp:5173 && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron .",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "sanity": "tsx src/scripts/sanity.ts",
    "test": "vitest run"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^3.23.8",
    "dexie": "^4.0.6",
    "nanoid": "^5.0.7",
    "react-router-dom": "^6.26.2",
    "@tanstack/react-query": "^5.59.16"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^30.0.0",
    "eslint": "^9.7.0",
    "eslint-plugin-react": "^7.35.0",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.7",
    "vite": "^5.4.0",
    "wait-on": "^7.2.0",
    "vitest": "^2.0.5",
    "@types/node": "^22.5.2",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "fake-indexeddb": "^6.0.0"
  }
}